{
    "name": "commercetools/symfony-bundle",
    "description": "Symfony bundle for Commercetools platform",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Commercetools\\Symfony\\CtpBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^3.1",
        "symfony/form": "^3.1",
        "symfony/security-bundle": "^3.1",
        "symfony/validator": "^3.1",
        "symfony/serializer": "^3.1",
        "symfony/cache": "^3.1",
        "commercetools/commons": "^0.2",
        "commercetools/php-sdk": "^1.5@dev",
        "symfony/var-dumper": "^3.1",
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 | ^5.0",
        "symfony/phpunit-bridge": "^3.0",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2-dev"
        }
    }
}
