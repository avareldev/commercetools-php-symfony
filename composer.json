{
    "name": "commercetools/symfony-bundle",
    "description": "Symfony bundle for Commercetools platform",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Commercetools\\Symfony\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^3.1 | ^4.0",
        "symfony/form": "^3.1  | ^4.0",
        "symfony/security-bundle": "^3.1 | ^4.0",
        "symfony/validator": "^3.1 | ^4.0",
        "symfony/serializer": "^3.1 | ^4.0",
        "symfony/cache": "^3.1 | ^4.0",
        "commercetools/commons": "^0.2",
        "commercetools/php-sdk": "dev-request_builder",
        "symfony/var-dumper": "^3.1 | ^4.0",
        "symfony/dotenv": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 | ^5.0",
        "symfony/phpunit-bridge": "^3.0 | ^4.0",
        "symfony/maker-bundle": "^1.4",
        "commercetools/api-reference": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2-dev"
        }
    }
}
